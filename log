  --color-main-dark: hsl(var(--color-hue),var(--color-saturation),calc(var(--color-lightning) - 10%));
  --color-main-light: hsl(var(--color-hue),var(--color-saturation),calc(var(--color-lightning) + 10%));
  --color-compliment: hsl(calc(var(--color-hue) + 180),var(--color-saturation),var(--color-lightning));
  --color-compliment-desaturate: hsl(calc(var(--color-hue) + 180),var(--color-saturation),calc(var(--color-lightning) + 40%));
